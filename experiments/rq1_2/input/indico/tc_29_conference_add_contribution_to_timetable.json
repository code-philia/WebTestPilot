{
  "name": "29::Conference Add Contribution to Timetable",
  "url": "http://localhost:8080/",
  "setup_function": "created_conference_page",
  "actions": [
    {
      "action": "Click \"Timetable\" link in navigation",
      "expectedResult": "Timetable view opens"
    },
    {
      "action": "Click \"Add new\" dropdown button",
      "expectedResult": "Add new menu appears"
    },
    {
      "action": "Click \"Contribution\" option",
      "expectedResult": "Contribution creation form opens"
    },
    {
      "action": "Click in the \"Title\" textbox",
      "expectedResult": "Title field is focused for input"
    },
    {
      "action": "Type \"Session Contribution\" in the title field",
      "expectedResult": "Title field contains unique contribution name"
    },
    {
      "action": "Click \"Save\" button",
      "expectedResult": "Contribution is created and saved"
    },
    {
      "action": "Verify contribution appears in timetable canvas with number prefix",
      "expectedResult": "Timetable shows numbered contribution"
    }
  ]
}