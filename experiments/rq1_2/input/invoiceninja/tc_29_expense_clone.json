{
  "name": "29::Expense Clone",
  "url": "http://localhost:8082/",
  "setup_function": "created_expense_page",
  "actions": [
    {
      "action": "Click \"More Actions\" dropdown button",
      "expectedResult": "Actions menu opens"
    },
    {
      "action": "Click \"Clone\" option",
      "expectedResult": "Clone action initiates"
    },
    {
      "action": "Click in the \"Amount\" textbox",
      "expectedResult": "Amount field is focused"
    },
    {
      "action": "Type \"23,2340\" in the amount field",
      "expectedResult": "Amount field contains value \"\"23,2340\""
    },
    {
      "action": "Click \"Save\" button",
      "expectedResult": "Cloned expense is created and saved"
    },
    {
      "action": "Verify cloned expense appears in expenses listing",
      "expectedResult": "New cloned expense is visible in expenses table"
    },
    {
      "action": "Verify expense has Logged status",
      "expectedResult": "The page shows expense's status is Logged"
    }
  ]
}