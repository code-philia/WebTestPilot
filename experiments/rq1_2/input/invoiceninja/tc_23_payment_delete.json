{
  "name": "23::Payment Delete",
  "url": "http://localhost:8082/",
  "setup_function": "created_payment_page",
  "actions": [
    {
      "action": "Click \"More Actions\" or \"Actions\" dropdown",
      "expectedResult": "Actions menu opens"
    },
    {
      "action": "Click \"Delete\" option",
      "expectedResult": "Delete confirmation dialog appears"
    },
    {
      "action": "Click \"Yes\" or \"Confirm\" button in the confirmation dialog",
      "expectedResult": "Payment deletion is confirmed"
    },
    {
      "action": "Verify alert message \"Successfully deleted payment\" appears",
      "expectedResult": "Success alert confirms payment deletion"
    },
    {
      "action": "Navigate to payments listing",
      "expectedResult": "Payments listing page is displayed"
    },
    {
      "action": "Verify deleted payment no longer appears in active list",
      "expectedResult": "Payment is removed from main listing"
    }
  ]
}