{
  "name": "32::Conference Registration Form Creation",
  "url": "http://localhost:8081/",
  "setup_function": "created_conference_page",
  "actions": [
    {
      "action": "Click \"Registration\" link in navigation",
      "expectedResult": "Registration section opens"
    },
    {
      "action": "Click \"Create form\" link",
      "expectedResult": "Registration form creation dialog opens"
    },
    {
      "action": "Click in the \"Title\" textbox",
      "expectedResult": "Title field is focused for input"
    },
    {
      "action": "Type \"Registration Form\" in the title field",
      "expectedResult": "Title field contains form name"
    },
    {
      "action": "Click \"Create\" button",
      "expectedResult": "Registration form is created"
    },
    {
      "action": "Verify alert message \"Registration form has been successfully created\" appears",
      "expectedResult": "Success alert confirms form creation"
    }
  ]
}